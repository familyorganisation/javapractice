package com.org;

public class Runner {

	public static void main(String[] args) {

		Parent parent = new Parent();
		parent.method();
		
		Child child = new Child();
		child.method();
		
		
		Parent parentInstance = new Child();
		parentInstance.method();
		
		parentInstance.otherMethod();
		
		parent = new ChildWithAccessModifiers();
		
		parent.method();
		
	}

}
